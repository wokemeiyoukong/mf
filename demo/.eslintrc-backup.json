{
  "extends": "standard",
  "plugins": ["@typescript-eslint", "prettier", "vue"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": [
      "@typescript-eslint/parser",
      "vue-eslint-parser",
      "babel-eslint"
    ],
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    "no-undef": "error",
    "space-before-function-paren": "off",
    "eol-last": "off"
  }
}

{
  "name": "vite-config",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "go": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.js,.ts,.jsx,.tsx src/",
